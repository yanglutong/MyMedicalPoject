<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activity.HomeLiNianActivity">

    <com.yang.mymedicalpoject.utils.MyTitleBar
        app:titleDrawable="@color/colorPrimary"
        android:id="@+id/myTitleBar"
        app:titleText="历年真题"
        app:titleSize="6sp"
        app:titleColor="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="-16dp"></com.yang.mymedicalpoject.utils.MyTitleBar>

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/tabLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myTitleBar"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="0dp"></com.google.android.material.tabs.TabLayout>

    <com.yang.mymedicalpoject.utils.MyViewPager
        android:id="@+id/liNiAn_Vp"
        android:layout_width="match_parent"
        android:layout_height="682dp"
        app:layout_constraintTop_toBottomOf="@+id/tabLayout"
        tools:layout_editor_absoluteX="0dp"></com.yang.mymedicalpoject.utils.MyViewPager>
</androidx.constraintlayout.widget.ConstraintLayout>